use_frameworks!

project 'YaDisk'
project 'YaAPI'

target 'YaDisk' do
  # Comment the next line if you don't want to use dynamic frameworks
  project 'YaDisk'

  # Pods for YaDisk
  pod 'Alamofire', '~> 5.6'
  pod 'SnapKit', '~> 5.6.0'

end

target 'YaAPI' do
  # Comment the next line if you don't want to use dynamic frameworks
  project 'YaAPI'

  # Pods for YaAPI
  pod 'Alamofire', '~> 5.6'

end